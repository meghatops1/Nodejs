<h1>View Product</h1>



  <table class="table">
    <thead>
      <tr>
        <th>Product Name</th>
        <th>Price</th>
        <th>Qty</th>
        <th>Image</th>
      </tr>
    </thead>
    <tbody>
      {{#each product}}
        <tr>
          <td>{{this.pname}}</td>
          <td>{{this.price}}</td>
          <td>{{this.qty}}</td>
          <td><img src="images/{{this.image}}" alt="" width="100px"></td>
        </tr>
      {{/each}}
    </tbody>
  </table>